{
  "name": "wiki-ai",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Fullstack app using React, MUI, Express, MongoDB",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server/index.js",
    "stop": "kill-port 3000 5000",
    "build": "cd client && npm run build",
    "lint": "eslint . --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "lint:client": "cd client && eslint src --ext .js,.jsx",
    "lint:server": "cd server && eslint . --ext .js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "test:firebase": "node scripts/test-firebase.js",
    "make:admin": "node scripts/make-admin.js",
    "db:init": "node scripts/init-database.js",
    "db:status": "node scripts/db-status.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.1",
    "helmet": "^7.2.0",
    "highlight.js": "^11.11.1",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^6.17.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "notistack": "^3.0.2",
    "openai": "^5.10.2",
    "pdf-parse": "^1.1.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  }
}
