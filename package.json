{
  "name": "wiki-ai",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Fullstack app using React, MUI, Express, MongoDB",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server/index.js",
    "stop": "kill-port 3000 5000",
    "build": "cd client && npm run build",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write .",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "test:firebase": "node scripts/test-firebase.js",
    "make:admin": "node scripts/make-admin.js",
    "db:init": "node scripts/init-database.js",
    "db:status": "node scripts/db-status.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^6.17.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.1",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  }
}
