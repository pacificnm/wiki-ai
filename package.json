{
  "name": "your-project-name",
  "version": "1.0.0",
  "private": true,
  "description": "Fullstack app using React, MUI, Express, MongoDB",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node backend/server.js",
    "stop": "kill-port 3000 5000",
    "build": "cd client && npm run build",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write .",
    "server": "nodemon backend/server.js",
    "client": "cd client && npm start"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "kill-port": "^2.0.1"
  }
}
